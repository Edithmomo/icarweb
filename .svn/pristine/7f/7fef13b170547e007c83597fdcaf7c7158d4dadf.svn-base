package com.ccunix.icar.employees.dao;

import java.util.List;

import com.ccunix.icar.employees.domain.Cart;

public interface CartDao {
	/**
	 * 录入汽车
	 * @param cart
	 * @return
	 */
	int insertCart(Cart cart);
	/**
	 * 根据车牌号查询
	 * @param cartId
	 * @return
	 */
	Cart queryCartByCartId(Cart cart);
	/**
	 * 修改
	 * @param cart
	 * @return
	 */
	int updateCart(Cart cart);
	/**
	 * 查询车辆信息
	 * @return
	 */
	List<Cart> queryCart();
}
